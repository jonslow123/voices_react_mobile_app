{
  "name": "voices-app",
  "expo": {
    "name": "Voices Radio",
    "slug": "voices-app",
    "entryPoint": "./index.js",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "scheme": "voicesradio",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/appicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#e5d7be"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.voices-app",
      "buildNumber": "1.0.0",
      "associatedDomains": [
        "applinks:auth.voicesradio.co.uk"
      ],
      "entitlements": {
        "aps-environment": "development"
      },
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSFaceIDUsageDescription": "This app may use Face ID to authenticate the user and auto-fill saved passwords."
      },
      "usesApnsToken": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon.png",
        "backgroundColor": "#e5d7be"
      },
      "package": "com.anonymous.voices-app",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.voicesradio.co.uk",
              "pathPrefix": "/verify-email"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": ["FOREGROUND_SERVICE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/appicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/appicon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ]
    ],
    "owner": "jonslow10",
    "extra": {
      "eas": {
        "projectId": "08fa2afe-af78-4e89-9035-d4d155cdfb66"
      }
    }
  }
}
